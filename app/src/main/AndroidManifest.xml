<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.testapp">

    <uses-permission android:name="android.permission.INTERNET"> </uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name=".utils.CustomApplication">
        <activity android:name=".SignUpActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".RegistrationActivity"> </activity>
        <activity android:name=".LoginActivity"> </activity>
        <activity android:name=".ForgotPasswordActivity"> </activity>
        <activity android:name=".ResetPasswordActivity"> </activity>
        <activity android:name=".StoreLocatorActivity"> </activity>
        <activity android:name=".StoreLocationsActivity"> </activity>
    </application>

</manifest>